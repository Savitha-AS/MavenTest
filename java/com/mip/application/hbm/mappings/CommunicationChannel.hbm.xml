<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mip.application.model.CommunicationChannel"
		table="communication_channel">
		<id name="chnId" type="int">
			<column name="chn_id" />
			<generator class="assigned" />
		</id>
		<property name="chnName" type="string">
			<column name="chn_name" length="50" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
